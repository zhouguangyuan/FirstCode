package ch2;

/**
 * @author Kevin Zhou
 * @date 2019年4月26日 下午5:27:23
 * 类说明：观察byte自动转换的操作
 * 要点说明：
 */
public class TestDemo12 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
     byte num = 100 ;	//100没有超过byte的保存范围
     System.out.println(num);	//输出byte变量的内容
     
	}

}
