package ch2;

/**
 * @author Kevin Zhou
 * @date 2019年4月26日 下午4:19:49
 * 类说明：扩大数据范围
 * 要点说明：
 */
public class TestDemo09 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int max = Integer.MAX_VALUE ;	//取出最大值
		int min = Integer.MIN_VALUE ;	//取出最小值
		
		System.out.println(max+1L);
		System.out.println(min-1L);
		
  
	}

}
